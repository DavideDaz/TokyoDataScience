    "def miniMaxSum(arr):\n",
    "    \n",
    "    no_max_arr = arr.copy()\n",
    "    no_min_arr = arr.copy()\n",
    "    \n",
    "    no_max_arr.remove(max(arr))\n",
    "    no_min_arr.remove(min(arr))\n",
    "    \n",
    "    print(str(sum(no_max_arr))+' '+str(sum(no_min_arr)))\n"
